<h1>Listing users</h1>

<%= will_paginate @users %>

<table>
  <tr>
    <th>Fname</th>
    <th>Lname</th>
    <th>Email</th>
    <th>Password</th>  
    <th>Dob</th>	
    <th>Age</th>
    <th>Gender</th>
    <th>Country</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.fname %></td>
    <td><%= user.lname %></td>
    <td><%= user.email %></td>
    <td><%= user.password %></td>  
	<td><%= user.dob %></td>
    <td><%= user.age %></td>
    <td><%= user.gender %></td>
    <td><%= user.country %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
